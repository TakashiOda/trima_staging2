            <div class="col-half">
              <%= f.label "経度" %>
              <%= f.text_field :latitude, class: "form-control", id: 'latitude_input' %>
            </div>
            <div class="col-half">
              <%= f.label "緯度" %>
              <%= f.text_field :longitude, class: "form-control", id: 'longitude_input' %>
            </div>
          </div>
        </div>
      </section>
