<div class="activity_index_top_box">
  <h3>体験を編集</h3>
</div>
<div class="activity_index_activity_list_box">
  <%= form_for [current_supplier, @activity] do |f| %>
    <%= render 'layouts/error_messages', model: f.object %>
    <div class="user_show_info_items_box">
      <section class="form_section">
        <div class="form_big_title">
          <h3>アクティビティの基本情報</h3>
        </div>
        <div class="activity_description_language_box">
          <ul class="activity_description_language_box_tab_list" role="tablist">
            <li id="japanese_tab" class="active" onclick="toggleDescriptionTab(this)">
              <span>日本語</span>
            </li>
            <li id="english_tab" onclick="toggleDescriptionTab(this)">
              <span>英語</span>
            </li>
          </ul>
          <div class="tab-content-items">
            <div id="japanese_tab_content">
              <div class="form-group">
                <%= f.label "体験名（日本語）" %>
                <%= f.text_field :name, required: true, class: "form-control" %>
              </div>
              <div class="form-group">
                <%= f.label "体験の紹介文（日本語）" %>
                <%= f.text_area :description, rows: 7, required: true, class: "form-control" %>
              </div>
              <div class="form-group">
                <%= f.label "注意事項・持ち物・装備など" %>
                <%= f.text_area :notes, rows: 7, class: "form-control" %>
              </div>
            </div>
            <div class="hidden" id="english_tab_content">
              <%= f.fields_for :activity_translations do |english| %>
                <%= english.hidden_field :language_id, value: 3, class: "form-control" %>
                <div class="form-group">
                  <%= english.label "体験名（英語）" %>
                  <%= english.text_field :name, required: true, class: "form-control" %>
                </div>
                <div class="form-group">
                  <%= english.label "体験の紹介文（英語）" %>
                  <%= english.text_area :profile_text, rows: 7, required: true, class: "form-control" %>
                </div>
                <div class="form-group">
                  <%= english.label "注意事項・持ち物・装備など" %>
                  <%= english.text_area :notes, rows: 7, class: "form-control" %>
                </div>
              <% end %>
            </div>
          </div>
        </div>

        <div class="form-group">
          <div class="row">
            <div class="col-half">
              <%= f.label "体験カテゴリー" %>
              <%= f.collection_select :activity_category_id, ActivityCategory.all, :id, :jp_name, {prompt: "選択してください"}, required: true, class: "form-control" %>
            </div>
          </div>
        </div>
      </section>
      <section class="form_section">
        <div class="form_big_title">
          <h3>参加条件</h3>
        </div>
        <div class="form-group">
          <div class="row">
            <div class="col-half">
              <%= f.label "参加できる最低年齢" %>
              <%= f.number_field :available_age, min: 0, max: 110, required: true, class: "form-control" %>
            </div>
          </div>
        </div>
        <div class="form-group">
          <div class="row">
            <div class="col-half">
              <%= f.label "最少催行人数" %>
              <%= f.number_field :minimum_num, min: 1, max: 49, required: true, class: "form-control" %>
            </div>
            <div class="col-half">
              <%= f.label "最大人数" %>
              <%= f.number_field :maximum_num, min: 1, max: 50, required: true, class: "form-control" %>
            </div>
          </div>
        </div>
      </section>
      <section class="form_section">
        <div class="form_big_title">
          <h3>アクティビティの位置情報</h3>
        </div>
