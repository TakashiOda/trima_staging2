        <%# <%= render 'activity_geolocation_input_map' %>
        <div class="form-group">
          <%= f.label "実施エリア" %>
          <%= f.collection_select :area_id, Area.all, :id, :local_name, {prompt: "選択してください"}, required: true, class: "form-control" %>
