<div class="nested-fields">
  <div class="form-group">
    <%= f.label "#{f.index}", class: "course_row1" %>
    <div class="course_row2 time_select">
      <%= f.time_select :start_hour, {:minute_step => 10},class: "form-control time_input" %>
    </div>
    <div class="course_row3">
      <span>qqqq</span>
    </div>
    <div class="course_row4">
      <%= link_to_remove_association f, class: "remove_course_btn" do %>
        <span><i class="fas fa-times"></i>削除</span>
      <% end %>
    </div>
  </div>
</div>
