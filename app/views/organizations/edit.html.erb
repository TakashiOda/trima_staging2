<div class="container user_show container-fluid row">
  <div class="user_show_side col-2">
    <%= render 'suppliers/supplier_page_front_sidebar' %>
  </div>
  <div class="user_show_content col-8">
    <!-- <form> -->
    <%= form_for [current_supplier, @organization] do |form| %>
    <div class="user_show_content_down">
      <div class="form_title">
        <h3>組織情報を編集</h3>
      </div>
      <%= render 'layouts/error_messages', model: form.object %>
      <%= render :partial => 'organizations/org_form', :locals => {:form => form} %>
    </div>
    <% end %>
    <!-- </form> -->
  </div>
</div>
